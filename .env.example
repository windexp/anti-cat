# Frigate 설정
FRIGATE_URL=http://your-frigate-ip:5000
FRIGATE_API_KEY=

# Gemini API 설정 (JSON 배열 형식, 여러 개 설정 가능)
# 예: GEMINI_API_KEYS=["key1", "key2", "key3"]
GEMINI_API_KEYS=["your-gemini-api-key"]

# 이벤트 폴링 설정
POLLING_INTERVAL_SECONDS=300
EVENT_MAX_AGE_DAYS=3
GEMINI_RETRY_INTERVAL_SECONDS=60
GEMINI_MAX_RETRIES=5

# 데이터 저장 경로
DATA_DIR=./data
IMAGES_DIR=./data/images
LABELS_DIR=./data/labels
MISMATCHED_DIR=./data/mismatched

# 웹서버 설정
WEB_HOST=0.0.0.0
WEB_PORT=8150
